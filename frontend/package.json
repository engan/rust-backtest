{
  "name": "vue-rust-backtest",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "build:wasm": "rm -rf src/rust/pkg && (cd ../ && wasm-pack build --target web && mv pkg frontend/src/rust/)"
  },
  "dependencies": {
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "lightweight-charts": "^5.0.8",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.3.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "~10.0.1",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.6",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
